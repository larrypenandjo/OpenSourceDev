<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="513.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="opensourcedev.controller.LoginWindowController">
   <children>
      <TextField fx:id="usernameField" alignment="CENTER" layoutX="155.0" layoutY="102.0" prefHeight="25.0" prefWidth="204.0" promptText="Username">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="155.0" layoutY="188.0" onKeyPressed="#loginOnKeyPressed" prefHeight="25.0" prefWidth="204.0" promptText="Password">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <Button layoutX="231.0" layoutY="298.0" mnemonicParsing="false" onAction="#loginBtnAction" text="Login" />
   </children>
</AnchorPane>
